<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BottomToolsView">
        <attr name="tabIconSize" format="reference|dimension"/>
        <attr name="tabIconTint" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="ThumbView">
        <attr name="like" format="reference|string"/>
        <attr name="is_liked" format="reference|boolean"/>
        <attr name="src" format="reference"/>
        <attr name="scale" format="float"/>
    </declare-styleable>


    <declare-styleable name="ProgressButton">
        <attr name="radius" format="reference|integer"/>
        <attr name="backgroundColor" format="reference|color"/>
        <attr name="text" format="reference|string"/>
        <attr name="progressImage" format="reference"/>
        <attr name="errorImage" format="reference"/>
        <attr name="doneImage" format="reference"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>

</resources>